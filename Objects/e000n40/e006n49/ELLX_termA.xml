<?xml version="1.0"?>

<PropertyList>

  <path>ELLX_termA.ac</path>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/bare</max-property>
  </animation>


  <animation>
    <type>material</type>
    <object-name>window_1</object-name>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.5</value>
      </greater-than>
    </condition>
    <emission>
      <red>   0.5 </red>
      <green> 0.5 </green>
      <blue>  0.5 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>window_1</object-name>
    <condition>
      <less-than-equals>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.50</value>
      </less-than-equals>
    </condition>
    <emission>
      <red>   0.0 </red>
      <green> 0.0 </green>
      <blue>  0.0 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>lower2</object-name>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.5</value>
      </greater-than>
    </condition>
    <emission>
      <red>   0.5 </red>
      <green> 0.5 </green>
      <blue>  0.5 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>lower2</object-name>
    <condition>
      <less-than-equals>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.50</value>
      </less-than-equals>
    </condition>
    <emission>
      <red>   0.0 </red>
      <green> 0.0 </green>
      <blue>  0.0 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>roof</object-name>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.5</value>
      </greater-than>
    </condition>
    <emission>
      <red>   0.1 </red>
      <green> 0.1 </green>
      <blue>  0.1 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>roof</object-name>
    <condition>
      <less-than-equals>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.50</value>
      </less-than-equals>
    </condition>
    <emission>
      <red>   0.0 </red>
      <green> 0.0 </green>
      <blue>  0.0 </blue>
    </emission>
  </animation>

</PropertyList>
