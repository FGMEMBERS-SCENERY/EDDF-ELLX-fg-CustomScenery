<?xml version="1.0"?>

<PropertyList>

  <path>ELLX_customs.ac</path>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/bare</max-property>
  </animation>


  <animation>
    <type>material</type>
    <object-name>Cube.001</object-name>
    <object-name>Cube.002</object-name>
    <object-name>Cube.0031</object-name>
    <object-name>Cube_0</object-name>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.50</value>
      </greater-than>
    </condition>
    <emission>
      <red>   0.3 </red>
      <green> 0.3 </green>
      <blue>  0.3 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>Cube.001</object-name>
    <object-name>Cube.002</object-name>
    <object-name>Cube.003</object-name>
    <object-name>Cube_0</object-name>
    <condition>
      <less-than-equals>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.50</value>
      </less-than-equals>
    </condition>
    <emission>
      <red>   0.0 </red>
      <green> 0.0 </green>
      <blue>  0.0 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>text</object-name>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.50</value>
      </greater-than>
    </condition>
    <emission>
      <red>   1.0 </red>
      <green> 1.0 </green>
      <blue>  1.0 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>text</object-name>
    <condition>
      <less-than-equals>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.50</value>
      </less-than-equals>
    </condition>
    <emission>
      <red>   0.5 </red>
      <green> 0.5 </green>
      <blue>  0.5 </blue>
    </emission>
  </animation>

</PropertyList>
